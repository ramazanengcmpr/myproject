package udemy;

import java.util.ArrayList;

public class araylis {
public  static void yazdir(ArrayList<String>a) {
	for (int i=0;i<a.size();i++) {
		System.out.println("element"+(i+9)+a.get(i));
	}
}
	public static void main(String[] args) {
		
		// TODO Auto-generated method stub
ArrayList<String>Arraylist=new ArrayList<String>();
System.out.println(Arraylist.add("dassault"));
System.out.println(Arraylist.add("rafale"));
System.out.println(Arraylist.add("FAD"));
System.out.println(Arraylist.size());
System.out.println(Arraylist.get(0));
Arraylist.remove(1);
 for (int i=0;i<Arraylist.size();i++);
 System.out.println(Arraylist.get(1));
 System.out.println(Arraylist.indexOf("fad"));
 yazdir(Arraylist);
 
 



	}

}
