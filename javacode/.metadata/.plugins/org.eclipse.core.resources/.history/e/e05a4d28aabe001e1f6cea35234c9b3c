package javalearn;

import java.util.Scanner;

public class ders12 {

	public static void main(String[] args) {
		
		Scanner scan=new Scanner(System.in);
		int[] sayi=new int[5];
		int toplam=0;
		for(int i=0;i<5;i++) {
			System.out.println("sayi griniz");
			sayi[i]=scan.nextInt();
			toplam=toplam+sayi[i];
		}
      System.out.println("toplam"+toplam);
	}

}
