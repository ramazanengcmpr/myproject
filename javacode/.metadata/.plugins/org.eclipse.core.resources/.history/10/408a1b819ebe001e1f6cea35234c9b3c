package javalearn;

public class der9try {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int star=1;
		
		for (int i=0;i<10;i++) {
			for(int j=0;j<star;j++) {
				System.out.println("*");
			}
			System.out.println("");
		}
		int star1=9;
		for (int i=0;i<10;i++) {
			for (int j=0;j<star1;j++) {
				
				System.out.println("*");
				
			}
			System.out.println("");
			star1--;
		}
		
	}

}
